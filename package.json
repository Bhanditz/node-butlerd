{
  "name": "node-buse",
  "version": "5.2.0",
  "description": "Node.js library for the butler API",
  "main": "index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run compile",
    "precompile": "tsc",
    "compile": "tsc && babel lib -d lib.development && cross-env NODE_ENV=production babel lib -d lib.production",
    "test": "npm run precompile && npm run tape",
    "tape": "node lib/tests/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itchio/node-buse.git"
  },
  "keywords": [
    "butler"
  ],
  "files": [
    "lib",
    "lib.development",
    "lib.production",
    "index.js"
  ],
  "author": "Amos Wenger <amos@itch.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itchio/node-buse/issues"
  },
  "homepage": "https://github.com/itchio/node-buse#readme",
  "devDependencies": {
    "@types/node": "^8.0.47",
    "@types/rimraf": "^2.0.2",
    "@types/split2": "^2.1.6",
    "@types/which": "^1.3.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "cross-env": "^5.1.4",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.2",
    "typescript": "^2.6.1",
    "which": "^1.3.0"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "split2": "^2.2.0",
    "uuid": "^3.2.1"
  }
}
